<launch>
  <node pkg="move_base" type="move_base" respawn="true" name="move_base" output="screen" clear_params="false">
    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/costmap_common_params_robot2.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/costmap_common_params_robot2.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/local_costmap_params_robot2.yaml" command="load" />
    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/global_costmap_params_robot2.yaml" command="load" />
    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/base_global_planner_param_robot2.yaml" command="load" />

    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/teb_local_planner_params_robot2.yaml" command="load" />
    <rosparam file="$(find xtark_nav)/config/omni/param_robot2_jianming/move_base_params_robot2.yaml" command="load" />
  </node>
  
</launch>
